{
  "plugins": ["transform-runtime", "transform-class-properties", "transform-object-rest-spread"],
  "presets": [
    ["env", { "modules": false }],
    "react"
  ],
  "env": {
    "test": {
      "plugins": ["transform-es2015-modules-commonjs"]
    },
    "production": {
      "plugins": ["transform-runtime", "transform-react-remove-prop-types"]
    }
  }
}
